{
  "name": "ousamma-survey",
  "version": "2.0.0",
  "description": "AI-Powered Survey Application with Advanced Features",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "start:survey-service": "cd services/survey-service && npm start",
    "dev:survey-service": "cd services/survey-service && npm run dev",
    "start:all": "concurrently \"npm start\" \"npm run start:survey-service\"",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:survey-service\"",
    "install:all": "npm install && cd services/survey-service && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.0.3",
    "ws": "^8.14.2",
    "redis": "^4.6.7",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-mongo-sanitize": "^2.2.0",
    "xss-clean": "^0.1.4",
    "hpp": "^0.2.3",
    "joi": "^17.11.0",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
