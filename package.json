{
  "name": "ousamma-survey",
  "version": "1.0.0",
  "description": "Survey platform with multi-provider AI service",
  "private": true,
  "workspaces": [
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=services/ai-service",
    "build": "npm run build --workspaces --if-present",
    "start": "npm run start --workspace=services/ai-service",
    "test": "npm run test --workspaces --if-present",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
