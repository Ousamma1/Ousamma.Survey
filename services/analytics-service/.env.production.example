# Analytics Service Production Configuration

NODE_ENV=production
PORT=3002
LOG_LEVEL=info

# Database
MONGODB_URI=mongodb://admin:password@mongodb:27017/analytics
MONGODB_DB_NAME=analytics

# Cache
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_TTL=3600
CACHE_WARMING_ENABLED=true
CACHE_TTL_HOT_DATA=300
CACHE_TTL_COLD_DATA=3600

# Kafka
KAFKA_BROKERS=kafka:9092
KAFKA_CLIENT_ID=analytics-service
KAFKA_GROUP_ID=analytics-group

# CORS
ALLOWED_ORIGINS=http://localhost:3000
