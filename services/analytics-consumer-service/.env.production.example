# Analytics Consumer Service Production Configuration

NODE_ENV=production
PORT=3003
LOG_LEVEL=info

# Database
MONGODB_URI=mongodb://admin:password@mongodb:27017/analytics
MONGODB_DB_NAME=analytics

# Cache
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_TTL=3600

# Kafka
KAFKA_BROKERS=kafka:9092
KAFKA_CLIENT_ID=analytics-consumer
KAFKA_GROUP_ID=analytics-consumer-group
KAFKA_TOPICS=response.submitted,survey.created,survey.updated

# Processing
BATCH_SIZE=100
BATCH_TIMEOUT=5000
