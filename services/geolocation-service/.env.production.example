# Geolocation Service Production Configuration

NODE_ENV=production
PORT=3001
LOG_LEVEL=info

# Database
MONGODB_URI=mongodb://admin:password@mongodb:27017/geolocation
MONGODB_DB_NAME=geolocation

# Geocoder
GEOCODER_PROVIDER=openstreetmap
GEOCODER_API_KEY=

# Configuration
DEFAULT_ACCURACY_THRESHOLD=100
MAX_LOCATION_HISTORY=1000

# CORS
ALLOWED_ORIGINS=http://localhost:3000
